{
  "name": "alfred-adcode",
  "version": "1.0.0",
  "description": "quick get adcode",
  "keywords": [
    "alfred",
    "adcode"
  ],
  "homepage": "https://github.com/daolou/alfred-adcode#readme",
  "bugs": {
    "url": "https://github.com/daolou/alfred-adcode/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daolou/alfred-adcode.git"
  },
  "license": "MIT",
  "author": "daolou <jiangzhiguo2010@qq.com>",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "lib",
    "scripts",
    "icon.png",
    "info.plist"
  ],
  "scripts": {
    "compile": "tsc",
    "eslint:fix": "eslint --fix --ext .ts ./src/",
    "postinstall": "alfy-init",
    "prepare": "husky install",
    "prettier:fix": "prettier --write -l \"./src/**/*\"",
    "publish": "./scripts/publish.sh",
    "release": "standard-version",
    "preuninstall": "alfy-cleanup"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{js,ts,md,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "alfy": "^1.0.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/node": "^16.11.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "devmoji": "^2.3.0",
    "eslint": "^8.14.0",
    "eslint-config-alloy": "^4.5.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sort-package-json": "^1.55.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
